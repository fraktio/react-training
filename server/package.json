{
  "name": "server",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "cross-env nodemon --ext ts,json --exec babel-node --extensions '.ts' src/index.ts",
    "test": "yarn lint && yarn tsc",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "build": "yarn build:clean && yarn build:babel",
    "build:babel": "babel src --out-dir build --copy-files --extensions='.ts'",
    "build:clean": "rm -rf build"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/faker": "^5.5.8",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4",
    "typescript": "^4.4.3"
  }
}
