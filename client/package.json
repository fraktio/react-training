    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^8.3.4",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.5.0",
    "msw": "^0.47.0",
