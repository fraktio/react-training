    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.34.2",
    "react-query": "4.0.0-beta.3",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.8.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.10.0",
    "@testing-library/jest-dom": "^5.16.5",
