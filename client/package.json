  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env TSC_COMPILE_ON_ERROR=true react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettify": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
